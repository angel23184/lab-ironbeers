<div class="card">
      <img class="card-img-top" src="{{this.image_url}}" alt="Card image beer">
      <div class="card-beer">
        <h3 class="beer-title">{{this.name}} </h3>
       <h4 class="beer-tagline">{{this.tagline}}</h4>
        <p class="card-text">{{this.description}}</p>
      </div>
</div>